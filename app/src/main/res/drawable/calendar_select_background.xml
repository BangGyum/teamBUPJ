<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
// 일정등록된 날짜 선택했을 때의 모형과 색상, 아이콘
    <item>
        <shape
            android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#A7E6EF">
            </stroke>
            <corners android:radius="2dp"></corners>
            <size
                android:width="16dp"
                android:height="16dp">
            </size>
        </shape>
    </item>
    <item
        android:width="10dp"
        android:height="10dp"
        android:bottom="2dp"
        android:drawable="@drawable/calendar_icon"
        android:gravity="bottom|center"></item>
</layer-list>